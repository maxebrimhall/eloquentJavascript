function range(start, end, step){
	let arr = []
	if(step === 0){
		step = 1
	}
	if(start < end){
		for(let i = start; i <= end; i += step){
		arr.push(i)
		}
	}else{
		for(let i = start; i >= end; i += step){
		arr.push(i)
		}
	}
	return(arr)
}

function sum(arr){
	let result = 0
	for(let i = 0; i < arr.length; i++){
		result += arr[i]
	}
	return result
}
